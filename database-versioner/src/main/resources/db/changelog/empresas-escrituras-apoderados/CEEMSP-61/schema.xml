<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd">

    <changeSet id="2022-08-10-agregar-empresas-apoderados-motivos-baja" author="carlos.garnica@pelisat.com.mx">
        <!-- This part will hold the catalogos tables -->
        <addColumn tableName="empresas_escrituras_apoderados">
            <column name="MOTIVO_BAJA" type="VARCHAR(60)"></column>
            <column name="FECHA_BAJA" type="DATE"></column>
            <column name="OBSERVACIONES_BAJA" type="TEXT"></column>
            <column name="DOCUMENTO_FUNDATORIO_BAJA" type="VARCHAR(255)"></column>
        </addColumn>
    </changeSet>
</databaseChangeLog>